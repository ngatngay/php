parameters:
    phpVersion: 70400
    level: 6
    paths:
        - src/
    reportUnmatchedIgnoredErrors: false
    ignoreErrors:
        -
            identifiers:
                - missingType.return
                - missingType.parameter
                - missingType.iterableValue
                - missingType.property
                - property.onlyWritten
                - missingType.generics
                - property.unused

